{
  "name": "jrdrwn.github.io",
  "version": "2.1.7",
  "description": "A personal website that I created when there was no project from the backend.",
  "keywords": [
    "blog",
    "tailwindcss"
  ],
  "homepage": "https://github.com/jrdrwn/jrdrwn.github.io#readme",
  "bugs": {
    "url": "https://github.com/jrdrwn/jrdrwn.github.io/issues",
    "email": "jordirwn@gmail.com"
  },
  "author": "Jordi Irawan <jordirwn@gmail.com> (https://jrdrwn.github.io/)",
  "license": "MIT",
  "main": "./public/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/jrdrwn/jrdrwn.github.io.git"
  },
  "scripts": {
    "dev": "npm run tailwind:watch",
    "prod": "npm run tailwind:minify && npm run minify",
    "tailwind:watch": "tailwindcss -i ./src/style.css -o ./public/css/style.css --watch",
    "tailwind:minify": "tailwindcss -i ./src/style.css -o ./public/css/style.css --minify",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "private": true,
  "dependencies": {
    "daisyui": "^2.15.3",
    "tailwindcss": "^3.1.2"
  }
}
